<launch>
  <arg name="manager" />
  <arg name="frame_topic" default="/hs/camera/image_raw" />
  <arg name="output_topic" default="/horned_sungem_nodelet/detected_objects" />
  <arg name="max_device_number" default="255" />
  <arg name="start_device_index" default="0" />
  <arg name="log_level" default="0" />
  <arg name="pixels" default="360" />
  <arg name="camera" default="hs"/>
  <arg name="cnn_type" default="mobilenetssd" />
  <arg name="param_file" default="$(find horned_sungem_launch)/config/mobilenetssd.yaml" />
  <arg name="top_n" default="1" />

  <node pkg="nodelet" type="nodelet" name="horned_sungem_stream"
        args="load horned_sungem_stream/HSNodelet $(arg manager)"
        output="screen">
      <remap from="/hs/camera/image_raw" to="$(arg frame_topic)" />
      <remap from="detected_objects" to="$(arg output_topic)" />
      <param name="max_device_number" value="$(arg max_device_number)" />
      <param name="start_device_index" value="$(arg start_device_index)" />
      <param name="log_level" value="$(arg log_level)" />
      <param name="pixels" value="$(arg pixels)"/>
      <param name="camera" value="$(arg camera)"/>
      <param name="cnn_type" value="$(arg cnn_type)"  />
      <param name="top_n" value="$(arg top_n)" />
      <rosparam command="load" file="$(arg param_file)" />
  </node>
</launch>

